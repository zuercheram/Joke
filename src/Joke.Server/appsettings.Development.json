{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(LocalDB)\\MSSQLLocalDB;Database=JokesApp;Trusted_Connection=True;Encrypt=False"
  },
  "https_port": 7202,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "HttpsInlineCertAndKeyFile": {
        "Url": "https://localhost:7202",
        "Certificate": {
          "Path": "../../etc/ssl/localhost.crt",
          "KeyPath": "../../etc/ssl/localhost.key",
          "Password": "1234"
        }
      }
    }
  },
  "SpaDevServerUrl": "https://localhost:7297",
  "ReverseProxy": {
    "Routes": {
      "index": {
        "ClusterId": "frontend",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "api": {
        "ClusterId": "backend",
        "Match": {
          "Path": "api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "frontend": {
        "HttpClient": {
          "SslProtocols": [ "Tls12" ]
        },
        "Destinations": {
          "cluster1/destination1": {
            "Address": "https://localhost:7297"
          }
        }
      },
      "backend": {
        "HttpClient": {
          "SslProtocols": [ "Tls12" ]
        },
        "Destinations": {
          "cluster1/destination1": {
            "Address": "https://localhost:7202"
          }
        }
      }
    }
  }
}
